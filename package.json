{
  "private": true,
  "name": "@meepshop/meep-lerna",
  "license": "ISC",
  "scripts": {
    "prettier": "prettier",
    "lerna": "lerna",
    "lerna-changelog": "lerna-changelog",
    "install:all": "npm install && lerna bootstrap",
    "postinstall": "patch-package",
    "clean:all": "make clean-all",
    "precommit": "lint-staged",
    "lint": "NODE_ENV=test esw --cache --ext .js",
    "lint:watch": "npm run lint -- -w --rule \"prettier/prettier: off\" --quiet",
    "dev": "lerna run dev --stream --parallel",
    "prod": "make prod",
    "release": "make release",
    "test": "NODE_ENV=test jest --silent"
  },
  "dependencies": {
    "antd": "^3.4.1",
    "radium": "^0.23.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.53",
    "@babel/core": "^7.0.0-beta.53",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.53",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.53",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.53",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.53",
    "@babel/polyfill": "^7.0.0-beta.53",
    "@babel/preset-env": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0-beta.53",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.0.1",
    "babel-plugin-css-modules-transform": "^1.6.1",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-imports": "^1.5.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.8.2",
    "eslint-watch": "^4.0.1",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "lerna": "^2.11.0",
    "lerna-changelog": "^0.8.0",
    "less": "^3.5.0-beta.7",
    "lint-staged": "^7.2.0",
    "next": "^6.0.3",
    "patch-package": "^5.1.1",
    "prettier": "^1.13.5"
  },
  "engines": {
    "node": ">=8.6.0",
    "npm": ">=5.3.0"
  }
}
