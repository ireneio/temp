{
  "private": true,
  "name": "@meepshop/front-end",
  "license": "ISC",
  "workspaces": ["packages/*", "store/*", "admin/*"],
  "scripts": {
    "admin": "make run-docker-compose TYPE=admin DOMAIN=admin.stage.meepcloud.com",
    "admin:old": "make run-docker-compose TYPE=old-admin DOMAIN=admin.stage.meepcloud.com",
    "apollo:watch": "make apollo-watch",
    "clean": "lerna clean && make clean",
    "dev": "make babel-changed WATCH=-w",
    "preinstall": "patch-package --reverse || echo \"no patch packages\"",
    "postinstall": "patch-package",
    "install:all": "yarn install && lerna bootstrap",
    "lint": "NODE_ENV=test esw --cache --ext js,ts,tsx",
    "lint:watch": "yarn lint -w --rule \"prettier/prettier: off\" --quiet",
    "prod": "NODE_ENV=production make babel-all",
    "release": "make release",
    "store": "make run-docker-compose TYPE=store DOMAIN=bellatest.stage.meepcloud.com",
    "storybook": "lerna run schema --stream --scope @meepshop/mock-types && NODE_ENV=test lerna exec ../../.storybook/index.js --stream --scope",
    "test": "lerna run schema --stream --scope @meepshop/mock-types && jest --silent --runInBand",
    "tsc": "make tsc",
    "tsc:watch": "make tsc-watch"
  },
  "dependencies": {
    "antd": "^3.10.1",
    "apollo": "^2.6.0",
    "apollo-boost": "^0.1.22",
    "graphql": "^14.2.1",
    "graphql-anywhere": "^4.1.24",
    "next": "^8.1.0",
    "prop-types": "^15.6.0",
    "react": "^16.4.2",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
    "@babel/plugin-proposal-decorators": "^7.0.0-rc.1",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-rc.1",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-rc.1",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.0.0-rc.1",
    "@storybook/addon-actions": "^5.0.1",
    "@storybook/addon-links": "^5.0.3",
    "@storybook/addon-viewport": "^5.0.1",
    "@storybook/react": "^5.0.0",
    "@types/clipboard": "^2.0.1",
    "@types/color": "^3.0.0",
    "@types/enzyme": "^3.9.0",
    "@types/execa": "^0.9.0",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^24.0.11",
    "@types/js-combinatorics": "^0.5.31",
    "@types/memoize-one": "^4.1.1",
    "@types/next": "^8.0.5",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@types/uuid": "^3.4.4",
    "@types/validator": "^10.11.0",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-css-modules-transform": "^1.6.1",
    "babel-plugin-idx": "^2.4.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-imports": "^1.5.0",
    "core-js": "^2.6.9",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "eslint-watch": "^4.0.2",
    "find-pkg-dir": "^1.0.1",
    "husky": "^1.1.0",
    "idx": "^2.5.5",
    "jest": "^24.5.0",
    "lerna": "^3.13.1",
    "lerna-changelog": "^0.8.0",
    "lint-staged": "^7.2.0",
    "output-file-sync": "^2.0.1",
    "patch-package": "^5.1.1",
    "postinstall-prepare": "^1.0.1",
    "prettier": "^1.13.5",
    "prettier-package-json": "^2.1.0",
    "react-markdown": "^4.0.6",
    "webpack": "^4.34.0"
  },
  "engines": {
    "node": ">=10.15.3",
    "npm": ">=5.3.0",
    "yarn": ">=1.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn tsc && lint-staged",
      "post-merge": "make babel-all",
      "post-checkout": "make babel-changed BRANCH=master"
    }
  },
  "resolutions": {
    "@types/query-string": "6.2.0",
    "@types/graphql": "14.2.3",
    "next-i18next": "0.37.0",
    "query-string": "5.1.1",
    "graphql": "14.2.1",
    "css-loader": "1.0.0",
    "less-loader": "5.0.0",
    "mini-css-extract-plugin": "0.4.3",
    "typescript": "3.5.3"
  }
}
