    "@zeit/next-less": "^1.0.1",
    "@zeit/next-source-maps": "0.0.3",
    "apollo": "^2.32.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-css-modules-transform": "^1.6.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-imports": "^2.0.0",
    "core-js": "^3.4.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-watch": "^6.0.1",
    "find-pkg-dir": "^2.0.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.2.0",
    "lerna": "^3.19.0",
    "less": "^3.10.3",
    "lint-staged": "^10.5.4",
    "lodash.kebabcase": "^4.1.1",
    "moment": "^2.29.1",
    "moment-locales-webpack-plugin": "^1.1.0",
    "output-file-sync": "^2.0.1",
    "patch-package": "^6.2.0",
    "postcss-less": "^3.1.4",
    "postinstall-prepare": "^1.0.1",
    "prettier": "^1.19.1",
    "prettier-package-json": "^2.1.3",
    "react-helmet": "^6.0.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "yo": "^3.1.1"
  },
  "engines": {
    "node": ">=10.15.3",
    "npm": ">=5.3.0",
    "yarn": ">=1.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "types codegen all && tsc && lint-staged",
      "post-merge": "make babel-all",
      "post-checkout": "make babel-all OPTION=\"--exclude-dependents --since master\""
    }
  },
  "resolutions": {
    "@types/query-string": "6.2.0",
    "@types/node": "13.11.1",
    "@types/react": "17.0.0",
    "apollo-link": "1.2.14",
    "bizcharts": "4.0.17",
    "query-string": "5.1.1",
    "url-parse": "1.4.7"
  }
}
