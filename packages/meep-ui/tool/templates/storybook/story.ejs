import React from 'react';
import { storiesOf } from '@storybook/react';

import Root from 'tool/storybook/components/Root';

<% if (hasCustomStory) { -%>
import Story from '..';
<% } else { -%>
import <%= displayName %> from '../..';
<% } -%>

const data = <%- JSON.stringify(storyData) %>;
<% if (!hasCustomStory) { -%>
const Story = props => <<%= displayName %> {...props} />;
<% } -%>

Story.displayName = Story.displayName || 'Story(<%= displayName %>)';

storiesOf('modules', module)
  .add('<%= displayName %>', () => (
    <Root
      displayName="<%= displayName[0].toLowerCase() + displayName.slice(1) %>"
      data={data}
    >
     <Story />
    </Root>
  ));
